{
  "name": "fico-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "start:qas": "env-cmd -f .env.qas craco start",
    "start:product": "env-cmd -f .env.product craco start",
    "build": "craco build",
    "build:develop": "env-cmd -f .env.develop yarn build",
    "build:qas": "env-cmd -f .env.qas yarn build",
    "build:product": "env-cmd -f .env.product yarn build",
    "deploy:develop": "bash deploy.dev.sh",
    "deploy:qas": "bash deploy.qas.sh",
    "deploy:product": "bash deploy.product.sh",
    "serve": "npx serve -s build --no-clipboard",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext ts,tsx src/",
    "lint:fix": "eslint --fix --ext ts,tsx src/",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss)\""
  },
